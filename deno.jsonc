{
  "tasks": {
    "cdk": "deno run --allow-all --allow-env npm:aws-cdk",
    "test": "deno test --allow-env --allow-read=__snapshots__,/tmp --allow-write=__snapshots__,/tmp",
    "ci": "deno fmt --check && deno lint && deno task test"
  },
  "lint": {
    "files": {
      "exclude": ["cdk.out"]
    }
  },
  "fmt": {
    "files": {
      "exclude": ["cdk.out"]
    }
  }
}
