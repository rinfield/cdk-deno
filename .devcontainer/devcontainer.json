{
  "name": "cdk-deno",
  "dockerFile": "Dockerfile",
  "context": ".",
  "extensions": [
    "denoland.vscode-deno"
  ],
  "settings": {
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": true,
    "deno.importMap": "importmap.json",
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  }
}
